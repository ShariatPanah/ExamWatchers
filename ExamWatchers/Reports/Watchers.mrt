		public string GetOrganName(byte organ)
		{
			switch(organ)
			{
				case 1:
					return "معاونت";
				
				case 2:
					return "پژوهشی";
				
				case 3:
					return "مالی";
				
				case 4:
					return "آموزش";
				
				default:
					return "معاونت";
			}
		}

